      </characteristic>
      <characteristic id="encoding_enable" name="Encoding Enable" sourceId="custom.type" uuid="00ce7a72-ec08-473d-943e-81ec27fdc603">
        <informativeText>Custom characteristic</informativeText>
        <value length="1" type="user" variable_length="false"/>
