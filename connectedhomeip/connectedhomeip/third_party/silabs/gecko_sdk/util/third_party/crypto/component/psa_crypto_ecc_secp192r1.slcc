id: psa_crypto_ecc_secp192r1
package: platform
label: secp192r1
description: >-
  This component adds support for the secp192r1 elliptic curve,
  a 192-bit prime field Weierstrass curve, in PSA Crypto.
category: Platform|Security|PSA Crypto|Elliptic Curves
quality: production
provides:
  - name: psa_crypto_ecc_secp192r1
  - name: psa_crypto_ecc_curve
    allow_multiple: true
requires:
  - name: psa_crypto_ecc
  - name: mbedtls_ecc_secp192r1
    condition: [device_series_0]
  - name: mbedtls_ecc_secp192r1
    condition: [device_series_1]
template_contribution:
  - name: psa_crypto_config
    value: PSA_WANT_ECC_SECP_R1_192
  - name: psa_crypto_config
    value: "MBEDTLS_PSA_BUILTIN_ECC_SECP_R1_192 1"
    condition: [mbedtls_ecc_secp192r1]
