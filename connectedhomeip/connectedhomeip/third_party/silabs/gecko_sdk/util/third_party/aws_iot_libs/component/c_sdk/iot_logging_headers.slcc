root_path: util/third_party/aws_iot_libs

include:
  - path: libraries/c_sdk/standard/common/include
    file_list:
      - path: iot_logging_setup.h
  - path: libraries/c_sdk/standard/common/include/private
    file_list:
      - path: iot_logging.h
